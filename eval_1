tasks = {}
def add_tasks(tasks, name, ID, status):
    if ID in tasks:
        print("this ID is already exists in tasks")
        return
    
    tasks[ID] = {
        "name": name,
        "task_status" : status,
    }
    return
def update_name(tasks,ID,name):
    if ID not in tasks:
        print("id does not exist")
        
    tasks[ID]["name"]= name
    print("sucessfully updated")
    
